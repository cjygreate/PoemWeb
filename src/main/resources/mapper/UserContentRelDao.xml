<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.verse.app.dao.UserContentRelDao">
    <insert id="insert" parameterType="com.verse.app.entity.UserContentRel">
        insert into user_content_rel
        (
            user_id,
            content_id,
            create_time.
            update_time
        )
        values
        (
            #{userId},
            #{contentId},
            NOW(),
            NOW()
        )
    </insert>
</mapper>