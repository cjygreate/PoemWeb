<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.verse.app.dao.PoetDao">
    <select id="queryPoetById" parameterType="java.lang.Long" resultType="com.verse.app.entity.Poet">
        select *
        from poet
        where id = #{id}
    </select>
</mapper>